<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ManageCategories">
   <children>
      <MenuBar fx:id="menuBar" layoutY="2.0" prefHeight="25.0" prefWidth="607.0">
        <menus>
          <Menu mnemonicParsing="false" text="Category">
            <items>
                <MenuItem fx:id="DeleteCategoryMenu" mnemonicParsing="false" onAction="#deleteCategory" text="Delete category" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="viewCategoryInfoButt" layoutX="399.0" layoutY="189.0" mnemonicParsing="false" onAction="#viewCategoryInfo" prefHeight="45.0" prefWidth="137.0" text="View Category Info" />
      <Button fx:id="updateCategoryButton" layoutX="398.0" layoutY="256.0" mnemonicParsing="false" onAction="#updateCategory" prefHeight="45.0" prefWidth="137.0" text="Update Category" />
      <Button fx:id="addCategoryButt" layoutX="399.0" layoutY="128.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="45.0" prefWidth="137.0" text="Add a category" />
      <TreeView fx:id="tree" layoutX="14.0" layoutY="32.0" prefHeight="336.0" prefWidth="306.0" />
      <Button fx:id="incomeButton" layoutX="354.0" layoutY="330.0" mnemonicParsing="false" onAction="#addIncome" prefHeight="33.0" prefWidth="89.0" text="Add income" />
      <Button fx:id="expenseButton" layoutX="467.0" layoutY="330.0" mnemonicParsing="false" onAction="#addExpense" prefHeight="33.0" prefWidth="89.0" text="Add expense" />
      <Label layoutX="336.0" layoutY="34.0" prefHeight="45.0" prefWidth="171.0" text="Total balance:" wrapText="true">
         <font>
            <Font size="26.0" />
         </font></Label>
      <Label fx:id="companyBalance" layoutX="336.0" layoutY="79.0" prefHeight="45.0" prefWidth="171.0" wrapText="true">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="14.0" layoutY="374.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
